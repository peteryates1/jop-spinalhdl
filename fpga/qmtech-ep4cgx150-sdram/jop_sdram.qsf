set_instance_assignment -name VIRTUAL_PIN ON -to stp_sdramBurstActive
set_instance_assignment -name VIRTUAL_PIN ON -to stp_sdramCtrlCmdValid
set_instance_assignment -name VIRTUAL_PIN ON -to stp_sdramCtrlCmdReady
set_instance_assignment -name VIRTUAL_PIN ON -to stp_sdramCtrlCmdWrite
set_instance_assignment -name VIRTUAL_PIN ON -to stp_sdramCtrlRspValid
set_instance_assignment -name VIRTUAL_PIN ON -to stp_sdramCtrlRspIsHigh
set_instance_assignment -name VIRTUAL_PIN ON -to stp_sdramLowHalfData[*]
set_instance_assignment -name VIRTUAL_PIN ON -to stp_hangDetected
set_instance_assignment -name VIRTUAL_PIN ON -to stp_pc[*]
